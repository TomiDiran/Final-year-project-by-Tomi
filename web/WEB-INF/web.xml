<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>resultprocessor</servlet-name>
        <servlet-class>resultprocessor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logver</servlet-name>
        <servlet-class>logver</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registration</servlet-name>
        <servlet-class>registration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>resultprocessor</servlet-name>
        <url-pattern>/resultprocessor</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MyResult</servlet-name>
        <servlet-class>com.assignment.career1.MyResult</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyResult</servlet-name>
        <url-pattern>/MyResult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logver</servlet-name>
        <url-pattern>/logver</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
